a:8:{s:5:"token";s:6:"b7e0a3";s:6:"parent";N;s:8:"children";a:0:{}s:4:"data";a:11:{s:6:"config";C:62:"Symfony\Component\HttpKernel\DataCollector\ConfigDataCollector":413:{a:16:{s:8:"app_name";N;s:11:"app_version";N;s:5:"token";s:6:"b7e0a3";s:15:"symfony_version";s:6:"2.6.13";s:4:"name";s:3:"n/a";s:3:"env";s:3:"n/a";s:5:"debug";s:3:"n/a";s:11:"php_version";s:6:"5.6.27";s:14:"xdebug_enabled";b:0;s:14:"eaccel_enabled";b:0;s:11:"apc_enabled";b:1;s:14:"xcache_enabled";b:0;s:16:"wincache_enabled";b:0;s:20:"zend_opcache_enabled";b:1;s:7:"bundles";a:0:{}s:9:"sapi_name";s:8:"fpm-fcgi";}}s:7:"request";C:63:"Symfony\Component\HttpKernel\DataCollector\RequestDataCollector":24635:{a:18:{s:6:"format";s:4:"html";s:7:"content";s:10673:"bolt_csrf_token=ab864750&title=Animacion+%23+2&slug=animacion-2&image%5Bfile%5D=2020-03%2Fcaptura2.png&teaser=%3C!DOCTYPE+html%3E%0D%0A%3Chtml+lang%3D%22en%22%3E%0D%0A++++%3Chead%3E%0D%0A++++++++%3Ctitle%3Ethree.js+webgl+-+postprocessing+backgrounds%3C%2Ftitle%3E%0D%0A++++++++%3Cmeta+charset%3D%22utf-8%22%3E%0D%0A++++++++%3Cmeta+name%3D%22viewport%22+content%3D%22width%3Ddevice-width%2C+user-scalable%3Dno%2C+minimum-scale%3D1.0%2C+maximum-scale%3D1.0%22%3E%0D%0A++++++++%3Clink+type%3D%22text%2Fcss%22+rel%3D%22stylesheet%22+href%3D%22main.css%22%3E%0D%0A++++%3C%2Fhead%3E%0D%0A++++%3Cbody%3E%0D%0A++++++++%3Cdiv+id%3D%22info%22%3E%0D%0A++++++++++++%3Ca+href%3D%22https%3A%2F%2Fthreejs.org%22+target%3D%22_blank%22+rel%3D%22noopener%22%3Ethree.js%3C%2Fa%3E+-+Backgrounds%3A+ClearPass%2C+TexturePass+and+CubeTexturePass%3Cbr%2F%3E%0D%0A++++++++++++by+%3Ca+href%3D%22https%3A%2F%2Fclara.io%22+target%3D%22_blank%22+rel%3D%22noopener%22%3EBen+Houston%3C%2Fa%3E%0D%0A++++++++%3C%2Fdiv%3E%0D%0A%0D%0A++++++++%3Cdiv+id%3D%22container%22%3E%3C%2Fdiv%3E%0D%0A%0D%0A++++++++%3Cscript+type%3D%22module%22%3E%0D%0A%0D%0A++++++++++++import+*+as+THREE+from+'..%2Fbuild%2Fthree.module.js'%3B%0D%0A%0D%0A++++++++++++import+Stats+from+'.%2Fjsm%2Flibs%2Fstats.module.js'%3B%0D%0A%0D%0A++++++++++++import+%7B+GUI+%7D+from+'.%2Fjsm%2Flibs%2Fdat.gui.module.js'%3B%0D%0A++++++++++++import+%7B+EffectComposer+%7D+from+'.%2Fjsm%2Fpostprocessing%2FEffectComposer.js'%3B%0D%0A++++++++++++import+%7B+RenderPass+%7D+from+'.%2Fjsm%2Fpostprocessing%2FRenderPass.js'%3B%0D%0A++++++++++++import+%7B+TexturePass+%7D+from+'.%2Fjsm%2Fpostprocessing%2FTexturePass.js'%3B%0D%0A++++++++++++import+%7B+CubeTexturePass+%7D+from+'.%2Fjsm%2Fpostprocessing%2FCubeTexturePass.js'%3B%0D%0A++++++++++++import+%7B+ShaderPass+%7D+from+'.%2Fjsm%2Fpostprocessing%2FShaderPass.js'%3B%0D%0A++++++++++++import+%7B+ClearPass+%7D+from+'.%2Fjsm%2Fpostprocessing%2FClearPass.js'%3B%0D%0A++++++++++++import+%7B+CopyShader+%7D+from+'.%2Fjsm%2Fshaders%2FCopyShader.js'%3B%0D%0A++++++++++++import+%7B+OrbitControls+%7D+from+'.%2Fjsm%2Fcontrols%2FOrbitControls.js'%3B%0D%0A%0D%0A++++++++++++var+scene%2C+renderer%2C+composer%3B%0D%0A++++++++++++var+clearPass%2C+texturePass%2C+renderPass%3B%0D%0A++++++++++++var+cameraP%2C+cubeTexturePassP%3B%0D%0A++++++++++++var+gui%2C+stats%3B%0D%0A%0D%0A++++++++++++var+params+%3D+%7B%0D%0A%0D%0A++++++++++++++++clearPass%3A+true%2C%0D%0A++++++++++++++++clearColor%3A+'white'%2C%0D%0A++++++++++++++++clearAlpha%3A+1.0%2C%0D%0A%0D%0A++++++++++++++++texturePass%3A+true%2C%0D%0A++++++++++++++++texturePassOpacity%3A+1.0%2C%0D%0A%0D%0A++++++++++++++++cubeTexturePass%3A+true%2C%0D%0A++++++++++++++++cubeTexturePassOpacity%3A+1.0%2C%0D%0A%0D%0A++++++++++++++++renderPass%3A+true%0D%0A++++++++++++%7D%3B%0D%0A%0D%0A++++++++++++init()%3B%0D%0A++++++++++++animate()%3B%0D%0A%0D%0A++++++++++++clearGui()%3B%0D%0A%0D%0A++++++++++++function+clearGui()+%7B%0D%0A%0D%0A++++++++++++++++if+(+gui+)+gui.destroy()%3B%0D%0A%0D%0A++++++++++++++++gui+%3D+new+GUI()%3B%0D%0A%0D%0A++++++++++++++++gui.add(+params%2C+%22clearPass%22+)%3B%0D%0A++++++++++++++++gui.add(+params%2C+%22clearColor%22%2C+%5B+'black'%2C+'white'%2C+'blue'%2C+'green'%2C+'red'+%5D+)%3B%0D%0A++++++++++++++++gui.add(+params%2C+%22clearAlpha%22%2C+0%2C+1+)%3B%0D%0A%0D%0A++++++++++++++++gui.add(+params%2C+%22texturePass%22+)%3B%0D%0A++++++++++++++++gui.add(+params%2C+%22texturePassOpacity%22%2C+0%2C+1+)%3B%0D%0A%0D%0A++++++++++++++++gui.add(+params%2C+%22cubeTexturePass%22+)%3B%0D%0A++++++++++++++++gui.add(+params%2C+%22cubeTexturePassOpacity%22%2C+0%2C+1+)%3B%0D%0A%0D%0A++++++++++++++++gui.add(+params%2C+%22renderPass%22+)%3B%0D%0A%0D%0A++++++++++++++++gui.open()%3B%0D%0A%0D%0A++++++++++++%7D%0D%0A%0D%0A++++++++++++function+init()+%7B%0D%0A%0D%0A++++++++++++++++var+container+%3D+document.getElementById(+%22container%22+)%3B%0D%0A%0D%0A++++++++++++++++var+width+%3D+window.innerWidth+%7C%7C+1%3B%0D%0A++++++++++++++++var+height+%3D+window.innerHeight+%7C%7C+1%3B%0D%0A++++++++++++++++var+aspect+%3D+width+%2F+height%3B%0D%0A++++++++++++++++var+devicePixelRatio+%3D+window.devicePixelRatio+%7C%7C+1%3B%0D%0A%0D%0A++++++++++++++++renderer+%3D+new+THREE.WebGLRenderer()%3B%0D%0A++++++++++++++++renderer.setPixelRatio(+devicePixelRatio+)%3B%0D%0A++++++++++++++++renderer.setSize(+width%2C+height+)%3B%0D%0A++++++++++++++++document.body.appendChild(+renderer.domElement+)%3B%0D%0A%0D%0A++++++++++++++++stats+%3D+new+Stats()%3B%0D%0A++++++++++++++++container.appendChild(+stats.dom+)%3B%0D%0A%0D%0A++++++++++++++++%2F%2F%0D%0A%0D%0A++++++++++++++++cameraP+%3D+new+THREE.PerspectiveCamera(+65%2C+aspect%2C+1%2C+10+)%3B%0D%0A++++++++++++++++cameraP.position.z+%3D+7%3B%0D%0A%0D%0A++++++++++++++++scene+%3D+new+THREE.Scene()%3B%0D%0A%0D%0A++++++++++++++++var+group+%3D+new+THREE.Group()%3B%0D%0A++++++++++++++++scene.add(+group+)%3B%0D%0A%0D%0A++++++++++++++++var+light+%3D+new+THREE.PointLight(+0xddffdd%2C+1.0+)%3B%0D%0A++++++++++++++++light.position.z+%3D+70%3B%0D%0A++++++++++++++++light.position.y+%3D+-+70%3B%0D%0A++++++++++++++++light.position.x+%3D+-+70%3B%0D%0A++++++++++++++++scene.add(+light+)%3B%0D%0A%0D%0A++++++++++++++++var+light2+%3D+new+THREE.PointLight(+0xffdddd%2C+1.0+)%3B%0D%0A++++++++++++++++light2.position.z+%3D+70%3B%0D%0A++++++++++++++++light2.position.x+%3D+-+70%3B%0D%0A++++++++++++++++light2.position.y+%3D+70%3B%0D%0A++++++++++++++++scene.add(+light2+)%3B%0D%0A%0D%0A++++++++++++++++var+light3+%3D+new+THREE.PointLight(+0xddddff%2C+1.0+)%3B%0D%0A++++++++++++++++light3.position.z+%3D+70%3B%0D%0A++++++++++++++++light3.position.x+%3D+70%3B%0D%0A++++++++++++++++light3.position.y+%3D+-+70%3B%0D%0A++++++++++++++++scene.add(+light3+)%3B%0D%0A%0D%0A++++++++++++++++var+geometry+%3D+new+THREE.SphereBufferGeometry(+1%2C+48%2C+24+)%3B%0D%0A%0D%0A++++++++++++++++var+material+%3D+new+THREE.MeshStandardMaterial()%3B%0D%0A++++++++++++++++material.roughness+%3D+0.5+*+Math.random()+%2B+0.25%3B%0D%0A++++++++++++++++material.metalness+%3D+0%3B%0D%0A++++++++++++++++material.color.setHSL(+Math.random()%2C+1.0%2C+0.3+)%3B%0D%0A%0D%0A++++++++++++++++var+mesh+%3D+new+THREE.Mesh(+geometry%2C+material+)%3B%0D%0A++++++++++++++++group.add(+mesh+)%3B%0D%0A%0D%0A++++++++++++++++%2F%2F+postprocessing%0D%0A%0D%0A++++++++++++++++var+genCubeUrls+%3D+function+(+prefix%2C+postfix+)+%7B%0D%0A%0D%0A++++++++++++++++++++return+%5B%0D%0A++++++++++++++++++++++++prefix+%2B+'px'+%2B+postfix%2C+prefix+%2B+'nx'+%2B+postfix%2C%0D%0A++++++++++++++++++++++++prefix+%2B+'py'+%2B+postfix%2C+prefix+%2B+'ny'+%2B+postfix%2C%0D%0A++++++++++++++++++++++++prefix+%2B+'pz'+%2B+postfix%2C+prefix+%2B+'nz'+%2B+postfix%0D%0A++++++++++++++++++++%5D%3B%0D%0A%0D%0A++++++++++++++++%7D%3B%0D%0A%0D%0A++++++++++++++++composer+%3D+new+EffectComposer(+renderer+)%3B%0D%0A%0D%0A++++++++++++++++clearPass+%3D+new+ClearPass(+params.clearColor%2C+params.clearAlpha+)%3B%0D%0A++++++++++++++++composer.addPass(+clearPass+)%3B%0D%0A%0D%0A++++++++++++++++texturePass+%3D+new+TexturePass()%3B%0D%0A++++++++++++++++composer.addPass(+texturePass+)%3B%0D%0A%0D%0A++++++++++++++++var+textureLoader+%3D+new+THREE.TextureLoader()%3B%0D%0A++++++++++++++++textureLoader.load(+%22textures%2Fhardwood2_diffuse.jpg%22%2C+function+(+map+)+%7B%0D%0A%0D%0A++++++++++++++++++++texturePass.map+%3D+map%3B%0D%0A%0D%0A++++++++++++++++%7D+)%3B%0D%0A%0D%0A++++++++++++++++cubeTexturePassP+%3D+null%3B%0D%0A%0D%0A++++++++++++++++var+ldrUrls+%3D+genCubeUrls(+%22textures%2Fcube%2Fpisa%2F%22%2C+%22.png%22+)%3B%0D%0A++++++++++++++++new+THREE.CubeTextureLoader().load(+ldrUrls%2C+function+(+ldrCubeMap+)+%7B%0D%0A%0D%0A++++++++++++++++++++cubeTexturePassP+%3D+new+CubeTexturePass(+cameraP%2C+ldrCubeMap+)%3B%0D%0A++++++++++++++++++++composer.insertPass(+cubeTexturePassP%2C+2+)%3B%0D%0A%0D%0A++++++++++++++++%7D+)%3B%0D%0A%0D%0A++++++++++++++++renderPass+%3D+new+RenderPass(+scene%2C+cameraP+)%3B%0D%0A++++++++++++++++renderPass.clear+%3D+false%3B%0D%0A++++++++++++++++composer.addPass(+renderPass+)%3B%0D%0A%0D%0A++++++++++++++++var+copyPass+%3D+new+ShaderPass(+CopyShader+)%3B%0D%0A++++++++++++++++composer.addPass(+copyPass+)%3B%0D%0A%0D%0A++++++++++++++++var+controls+%3D+new+OrbitControls(+cameraP%2C+renderer.domElement+)%3B%0D%0A%0D%0A++++++++++++++++window.addEventListener(+'resize'%2C+onWindowResize%2C+false+)%3B%0D%0A%0D%0A++++++++++++%7D%0D%0A%0D%0A++++++++++++function+onWindowResize()+%7B%0D%0A%0D%0A++++++++++++++++var+width+%3D+window.innerWidth%3B%0D%0A++++++++++++++++var+height+%3D+window.innerHeight%3B%0D%0A++++++++++++++++var+aspect+%3D+width+%2F+height%3B%0D%0A%0D%0A++++++++++++++++cameraP.aspect+%3D+aspect%3B%0D%0A++++++++++++++++cameraP.updateProjectionMatrix()%3B%0D%0A%0D%0A++++++++++++++++renderer.setSize(+width%2C+height+)%3B%0D%0A++++++++++++++++composer.setSize(+width%2C+height+)%3B%0D%0A%0D%0A++++++++++++%7D%0D%0A%0D%0A++++++++++++function+animate()+%7B%0D%0A%0D%0A++++++++++++++++requestAnimationFrame(+animate+)%3B%0D%0A%0D%0A++++++++++++++++stats.begin()%3B%0D%0A%0D%0A++++++++++++++++cameraP.updateMatrixWorld(+true+)%3B%0D%0A%0D%0A++++++++++++++++var+newColor+%3D+clearPass.clearColor%3B%0D%0A++++++++++++++++switch+(+params.clearColor+)+%7B%0D%0A%0D%0A++++++++++++++++++++case+'blue'%3A+newColor+%3D+0x0000ff%3B+break%3B%0D%0A++++++++++++++++++++case+'red'%3A+newColor+%3D+0xff0000%3B+break%3B%0D%0A++++++++++++++++++++case+'green'%3A+newColor+%3D+0x00ff00%3B+break%3B%0D%0A++++++++++++++++++++case+'white'%3A+newColor+%3D+0xffffff%3B+break%3B%0D%0A++++++++++++++++++++case+'black'%3A+newColor+%3D+0x000000%3B+break%3B%0D%0A%0D%0A++++++++++++++++%7D%0D%0A%0D%0A++++++++++++++++clearPass.enabled+%3D+params.clearPass%3B%0D%0A++++++++++++++++clearPass.clearColor+%3D+newColor%3B%0D%0A++++++++++++++++clearPass.clearAlpha+%3D+params.clearAlpha%3B%0D%0A%0D%0A++++++++++++++++texturePass.enabled+%3D+params.texturePass%3B%0D%0A++++++++++++++++texturePass.opacity+%3D+params.texturePassOpacity%3B%0D%0A%0D%0A++++++++++++++++if+(+cubeTexturePassP+!%3D+null+)+%7B%0D%0A++++++++++++++++++++cubeTexturePassP.enabled+%3D+params.cubeTexturePass%3B%0D%0A++++++++++++++++++++cubeTexturePassP.opacity+%3D+params.cubeTexturePassOpacity%3B%0D%0A++++++++++++++++%7D%0D%0A%0D%0A++++++++++++++++renderPass.enabled+%3D+params.renderPass%3B%0D%0A%0D%0A++++++++++++++++composer.render()%3B%0D%0A%0D%0A++++++++++++++++stats.end()%3B%0D%0A%0D%0A++++++++++++%7D%0D%0A%0D%0A++++++++%3C%2Fscript%3E%0D%0A++++%3C%2Fbody%3E%0D%0A%3C%2Fhtml%3E&body=&template=&taxonomy%5Bchapters%5D%5B%5D=&taxonomy-order%5Bchapters%5D=0&editreferrer=%2Fbolt%2Foverview%2Fpages&id=4&contenttype=pages&datepublish=2020-03-02+22%3A12%3A52&datedepublish=&status=draft&ownerid=1&_live-editor-preview=";s:12:"content_type";s:16:"application/json";s:11:"status_text";s:2:"OK";s:11:"status_code";i:200;s:13:"request_query";a:1:{s:8:"returnto";s:4:"ajax";}s:15:"request_request";a:17:{s:15:"bolt_csrf_token";s:8:"ab864750";s:5:"title";s:13:"Animacion # 2";s:4:"slug";s:11:"animacion-2";s:5:"image";a:1:{s:4:"file";s:20:"2020-03/captura2.png";}s:6:"teaser";s:8361:"<!DOCTYPE html>
<html lang="en">
    <head>
        <title>three.js webgl - postprocessing backgrounds</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
        <link type="text/css" rel="stylesheet" href="main.css">
    </head>
    <body>
        <div id="info">
            <a href="https://threejs.org" target="_blank" rel="noopener">three.js</a> - Backgrounds: ClearPass, TexturePass and CubeTexturePass<br/>
            by <a href="https://clara.io" target="_blank" rel="noopener">Ben Houston</a>
        </div>

        <div id="container"></div>

        <script type="module">

            import * as THREE from '../build/three.module.js';

            import Stats from './jsm/libs/stats.module.js';

            import { GUI } from './jsm/libs/dat.gui.module.js';
            import { EffectComposer } from './jsm/postprocessing/EffectComposer.js';
            import { RenderPass } from './jsm/postprocessing/RenderPass.js';
            import { TexturePass } from './jsm/postprocessing/TexturePass.js';
            import { CubeTexturePass } from './jsm/postprocessing/CubeTexturePass.js';
            import { ShaderPass } from './jsm/postprocessing/ShaderPass.js';
            import { ClearPass } from './jsm/postprocessing/ClearPass.js';
            import { CopyShader } from './jsm/shaders/CopyShader.js';
            import { OrbitControls } from './jsm/controls/OrbitControls.js';

            var scene, renderer, composer;
            var clearPass, texturePass, renderPass;
            var cameraP, cubeTexturePassP;
            var gui, stats;

            var params = {

                clearPass: true,
                clearColor: 'white',
                clearAlpha: 1.0,

                texturePass: true,
                texturePassOpacity: 1.0,

                cubeTexturePass: true,
                cubeTexturePassOpacity: 1.0,

                renderPass: true
            };

            init();
            animate();

            clearGui();

            function clearGui() {

                if ( gui ) gui.destroy();

                gui = new GUI();

                gui.add( params, "clearPass" );
                gui.add( params, "clearColor", [ 'black', 'white', 'blue', 'green', 'red' ] );
                gui.add( params, "clearAlpha", 0, 1 );

                gui.add( params, "texturePass" );
                gui.add( params, "texturePassOpacity", 0, 1 );

                gui.add( params, "cubeTexturePass" );
                gui.add( params, "cubeTexturePassOpacity", 0, 1 );

                gui.add( params, "renderPass" );

                gui.open();

            }

            function init() {

                var container = document.getElementById( "container" );

                var width = window.innerWidth || 1;
                var height = window.innerHeight || 1;
                var aspect = width / height;
                var devicePixelRatio = window.devicePixelRatio || 1;

                renderer = new THREE.WebGLRenderer();
                renderer.setPixelRatio( devicePixelRatio );
                renderer.setSize( width, height );
                document.body.appendChild( renderer.domElement );

                stats = new Stats();
                container.appendChild( stats.dom );

                //

                cameraP = new THREE.PerspectiveCamera( 65, aspect, 1, 10 );
                cameraP.position.z = 7;

                scene = new THREE.Scene();

                var group = new THREE.Group();
                scene.add( group );

                var light = new THREE.PointLight( 0xddffdd, 1.0 );
                light.position.z = 70;
                light.position.y = - 70;
                light.position.x = - 70;
                scene.add( light );

                var light2 = new THREE.PointLight( 0xffdddd, 1.0 );
                light2.position.z = 70;
                light2.position.x = - 70;
                light2.position.y = 70;
                scene.add( light2 );

                var light3 = new THREE.PointLight( 0xddddff, 1.0 );
                light3.position.z = 70;
                light3.position.x = 70;
                light3.position.y = - 70;
                scene.add( light3 );

                var geometry = new THREE.SphereBufferGeometry( 1, 48, 24 );

                var material = new THREE.MeshStandardMaterial();
                material.roughness = 0.5 * Math.random() + 0.25;
                material.metalness = 0;
                material.color.setHSL( Math.random(), 1.0, 0.3 );

                var mesh = new THREE.Mesh( geometry, material );
                group.add( mesh );

                // postprocessing

                var genCubeUrls = function ( prefix, postfix ) {

                    return [
                        prefix + 'px' + postfix, prefix + 'nx' + postfix,
                        prefix + 'py' + postfix, prefix + 'ny' + postfix,
                        prefix + 'pz' + postfix, prefix + 'nz' + postfix
                    ];

                };

                composer = new EffectComposer( renderer );

                clearPass = new ClearPass( params.clearColor, params.clearAlpha );
                composer.addPass( clearPass );

                texturePass = new TexturePass();
                composer.addPass( texturePass );

                var textureLoader = new THREE.TextureLoader();
                textureLoader.load( "textures/hardwood2_diffuse.jpg", function ( map ) {

                    texturePass.map = map;

                } );

                cubeTexturePassP = null;

                var ldrUrls = genCubeUrls( "textures/cube/pisa/", ".png" );
                new THREE.CubeTextureLoader().load( ldrUrls, function ( ldrCubeMap ) {

                    cubeTexturePassP = new CubeTexturePass( cameraP, ldrCubeMap );
                    composer.insertPass( cubeTexturePassP, 2 );

                } );

                renderPass = new RenderPass( scene, cameraP );
                renderPass.clear = false;
                composer.addPass( renderPass );

                var copyPass = new ShaderPass( CopyShader );
                composer.addPass( copyPass );

                var controls = new OrbitControls( cameraP, renderer.domElement );

                window.addEventListener( 'resize', onWindowResize, false );

            }

            function onWindowResize() {

                var width = window.innerWidth;
                var height = window.innerHeight;
                var aspect = width / height;

                cameraP.aspect = aspect;
                cameraP.updateProjectionMatrix();

                renderer.setSize( width, height );
                composer.setSize( width, height );

            }

            function animate() {

                requestAnimationFrame( animate );

                stats.begin();

                cameraP.updateMatrixWorld( true );

                var newColor = clearPass.clearColor;
                switch ( params.clearColor ) {

                    case 'blue': newColor = 0x0000ff; break;
                    case 'red': newColor = 0xff0000; break;
                    case 'green': newColor = 0x00ff00; break;
                    case 'white': newColor = 0xffffff; break;
                    case 'black': newColor = 0x000000; break;

                }

                clearPass.enabled = params.clearPass;
                clearPass.clearColor = newColor;
                clearPass.clearAlpha = params.clearAlpha;

                texturePass.enabled = params.texturePass;
                texturePass.opacity = params.texturePassOpacity;

                if ( cubeTexturePassP != null ) {
                    cubeTexturePassP.enabled = params.cubeTexturePass;
                    cubeTexturePassP.opacity = params.cubeTexturePassOpacity;
                }

                renderPass.enabled = params.renderPass;

                composer.render();

                stats.end();

            }

        </script>
